{
    "name": "dome/ares-habbo-api",
    "description": "Fast and convenient Habbo API",
    "keywords": [
        "php",
        "habbo",
        "api",
        "slim"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dome",
            "homepage": "https://ares.to"
        }
    ],
    "require": {
        "ext-json": "*",
        "doctrine/orm": "^2.7.3",
        "lcobucci/jwt": "^3.3.2",
        "league/container": "dev-master",
        "monolog/monolog": "^1.24",
        "nesbot/carbon": "^2.0@dev",
        "phlak/config": "dev-master",
        "respect/validation": "dev-master",
        "slim/psr7": "^0.5.0",
        "slim/slim": "^4.5.0",
        "symfony/polyfill-uuid": "^1.17@dev",
        "tuupola/cors-middleware": "dev-master",
        "vlucas/phpdotenv": "^5.0@dev"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
