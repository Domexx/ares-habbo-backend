{
    "name": "dome/slim4-habbo-api",
    "description": "this is an api for a habbo project",
    "keywords": [
        "php",
        "habbo",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dome",
            "homepage": "https://ares.to"
        }
    ],
    "require": {
        "ext-json": "*",
        "doctrine/orm": "^2.7.3",
        "slim/psr7": "^0.5.0",
        "slim/slim": "^4.5.0",
        "monolog/monolog": "^1.24",
        "php-di/php-di": "^6.0",
        "vlucas/phpdotenv": "^2.4",
        "tuupola/slim-jwt-auth": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/console": "^4.2"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
