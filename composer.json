{
    "name": "dome/ares-habbo-api",
    "description": "Fast and convenient Habbo API",
    "keywords": [
        "php",
        "habbo",
        "api",
        "slim"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dome",
            "homepage": "https://ares.to"
        },
        {
            "name": "s1njar",
            "homepage": "https://ares.to"
        }
    ],
    "require": {
        "ext-json": "*",
        "ext-sockets": "*",
        "cocur/slugify": "dev-master",
        "illuminate/database": "^9.0@dev",
        "illuminate/pagination": "^9.0@dev",
        "league/container": "3.3.3",
        "monolog/monolog": "^1.24",
        "phlak/config": "dev-master",
        "phpfastcache/phpfastcache": "dev-master",
        "predis/predis": "^2.0@dev",
        "rakit/validation": "dev-master",
        "rbdwllr/psr-jwt": "dev-master",
        "slim/psr7": "^0.5.0",
        "slim/slim": "^4.5.0",
        "statical/statical": "dev-master",
        "tuupola/cors-middleware": "dev-master",
        "vlucas/phpdotenv": "^5.0@dev"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    },
    "autoload": {
        "psr-4": {
            "Ares\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
